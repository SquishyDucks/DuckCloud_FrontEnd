{{#each uploads as |upload|}}
  <tr class="file-row" data-id="{{upload._id}}" data-owner="{{upload.owner}}">
    <td class="file-title"><input class="file-table-cell-edit file-table-cell-inactive" value="{{upload.title}}" disabled="disabled"/></td>
    <td>{{upload.createdAt}}</td>
    <td>{{upload.updatedAt}}</td>
    <td class="file-tags"><input class="file-table-cell-edit file-table-cell-inactive" value="{{upload.tags}}" disabled="disabled"/></td>
    <td><button type="button" class="btn-line clickable editable-btn edit-btn">Edit</button></td>
    <td><button type="button" class="btn-line clickable editable-btn update-btn hide">Update</button></td>
    <td><button type="button" class="btn-line clickable editable-btn delete-btn">Delete</button></td>
    <td><button type="button" class="btn-line clickable view-btn">View</button></td>
  </tr>

{{/each}}
